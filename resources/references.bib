	pages = {123--140},
	file = {Fulltekst:C\:\\Users\\torkilr\\Zotero\\storage\\WXC86AYJ\\Fryer et al. - 1995 - Effects of creatine phosphate and P(i) on Ca2+ mov.pdf:application/pdf},
}

