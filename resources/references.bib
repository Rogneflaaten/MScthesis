	author = {Lamb, G D and Stephenson, D G},
	month = mar,
	year = {1991},
	pages = {507--528},
	file = {Fulltekst:C\:\\Users\\torkilr\\Zotero\\storage\\7YSSREUM\\Lamb og Stephenson - 1991 - Effect of Mg2+ on the control of Ca2+ release in s.pdf:application/pdf},
}

